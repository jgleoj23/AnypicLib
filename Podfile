    pod 'ParseUI'
    pod 'MBProgressHUD', '~> 0.8'
    pod 'Reachability'
    pod 'FormatterKit'
    pod 'UIImageEffects'
end

target 'AnypicLib' do
    pod 'Parse'
    pod 'ParseFacebookUtilsV4'
    pod 'ParseCrashReporting'
    pod 'ParseUI'
    pod 'MBProgressHUD', '~> 0.8'
    pod 'Reachability'
    pod 'FormatterKit'
    pod 'UIImageEffects'
end